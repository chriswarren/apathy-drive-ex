<%= render ApathyDrive.PageView, "nav.html", conn: @conn %>

<table>
  <tr>
    <th>Name</th>
    <td><%= @class.name %></td>
  </tr>
  <tr>
    <th>Alignment</th>
    <td><%= @class.alignment %></td>
  </tr>
</table>

<%= link "Edit", to: class_path(@conn, :edit, @class) %>
<%= link "Back", to: class_path(@conn, :index) %>
